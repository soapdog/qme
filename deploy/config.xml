    <gap:plugin name="com.pushwoosh.plugins.pushwoosh" version="3.0.2" />
    <gap:plugin name="com.phonegap.plugins.facebookconnect" version="0.4.0">
        <param name="APP_ID" value="135598219957283" />
        <param name="APP_NAME" value="Megapix Avisa" />
    </gap:plugin>
    <nofeature name="PushNotification">
        <param name="ios-package" value="PushNotification" />
    </nofeature>
